@ARTICLE{Ángel-Beamonte2018605,
author={Ángel-Beamonte, E. and Martín-Ramos, P. and Santolaria, P. and Sales, E. and Abizanda, J. and Yániz, J.L.},
title={Automatic determination of landmark coordinates for honey bee forewing venation using a new MATLAB-based tool},
journal={Journal of Apicultural Research},
year={2018},
