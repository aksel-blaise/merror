@ARTICLE{Leonard2018,
author={Leonard, R.J. and Wat, K.K.Y. and McArthur, C. and Hochuli, D.F.},
title={Urbanisation and wing asymmetry in the western honey bee (Apis mellifera, Linnaeus 1758) at multiple scales},
journal={PeerJ},
year={2018},
volume={2018},
number={12},
doi={10.7717/peerj.5940},
